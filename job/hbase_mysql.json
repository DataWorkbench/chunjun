              },
              {
                "name": "ename",
                "type": "VARCHAR"
              },
              {
                "name": "job",
                "type": "VARCHAR"
              }
            ],
            "connection": [
              {
                "jdbcUrl": "jdbc:mysql://hadoop1:3306/test?useSSL=false",
                "table": [
                  "emp"
                ]
              }
            ],
            "password": "000000",
            "username": "root"
          }
        }
      }
    ]
  }
